<div class="row">
	<div class="span8">
		<div class="well">
			<h2>Member: <%= @user.name %></h2>
			<p><%= image_tag @user.avatar.url(:thumb) %></p>
			
<%= link_to 'Back', dog_walks_path %>

<h4>Dog Walks by Member</h2>
  <ul>
<% @user.dog_walks.each do |dog_walk| %>
<li>Town: <%= dog_walk.town %> | County: <%= dog_walk.county %> | Distance <%= dog_walk.distance %>km <%= link_to 'View Walk', dog_walk %><% if current_user == dog_walk.user %> | <%= link_to 'Edit Walk', edit_dog_walk_path(dog_walk) %> | <%= link_to 'Delete Walk', dog_walk, method: :delete, data: { confirm: 'Are you sure?' } %><% end %></li>
<% end %>
</ul>
</div>
</div>
</div>
