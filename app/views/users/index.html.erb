<table class="table table-striped">
  <thead>
	<th>User Name</th>
    <th>Avatar</th>
   <th>Number of Walks</th>
  </thead>
<tbody>
<% @user.each do |user| %>
  <tr>
    <td><%= link_to user.name, user_path(user.id)  %></td>
    <td><%= image_tag user.avatar(:thumb) %></td>
    <td><%= user.dog_walks.count.to_s %></td>
</tr>
<% end %>
 </tbody>
</table>

<br />